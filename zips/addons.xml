<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.tgg" name="[B][COLOR teal]TGG Wizard[/COLOR][/B] Wizard" version="1.0" provider-name="unknownTGG">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[B][COLOR teal]TGG Wizard[/COLOR][/B] Wizard</summary>
    <description lang="en">[B][COLOR teal]TGG Wizard[/COLOR][/B] offers many maintenance features, selective add-on data saving, and automated build installs.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.program.tgg" name="[B][COLOR teal]TGG Wizard[/COLOR][/B]" version="1.0" provider-name="unknownTGG">
    <extension point="xbmc.addon.repository" name="TGG Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/unknownTGG/repo.tggwizard/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/unknownTGG/repo.tggwizard/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/unknownTGG/repo.tggwizard/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>[B][COLOR teal]TGG Wizard[/COLOR][/B]</summary>
        <description>Fork of Aftermath Wizard, containing [B][COLOR teal]SpecialK[/COLOR] build[/B]</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
