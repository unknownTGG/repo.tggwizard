<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.tgg" name="[COLOR teal]TGG Wizard[/COLOR]" version="1.2.2" provider-name="unknownTGG">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[B][COLOR teal]TGG Wizard[/COLOR][/B]</summary>
    <description lang="en">Fork of Aftermath Wizard, containing [B][COLOR teal]SpecialK[/COLOR] build[/B]</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.tgg" name="[COLOR teal]TGG Repository[/COLOR]" version="1.2.1" provider-name="unknownTGG">
    <extension point="xbmc.addon.repository" name="TGG Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/unknownTGG/unknowntgg.github.io/master/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/unknownTGG/unknowntgg.github.io/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/unknownTGG/unknowntgg.github.io/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>[COLOR teal]TGG Repository[/COLOR]</summary>
        <description>The official TGG Repository.</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
